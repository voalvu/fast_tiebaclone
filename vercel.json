{
    "version": 2,
    "builds": [
      {
        "src": "build.sh",
        "use": "@vercel/static-build",
        "config": {
          "buildCommand": "chmod +x build.sh && ./build.sh"
        }
      }
    ],
    "functions": {
      "public/index.sh": {
        "memory": 4096,
        "maxDuration": 60,
        "runtime": "provided"
      }
    },
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/public/index.sh"
      }
    ],
    "env": {
      "NODE_ENV": "production",
      "GIT_TOKEN": ""
    }
  }