{
    "version": 2,
    "functions": {
        "api/index.js": {
            "runtime": "edge"
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/index.js"
        }
    ],
    "builds": [
        {
            "src": "api/tieba.js",
            "use": "@vercel/static"
        }
    ]
}