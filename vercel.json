{
    "version": 2,
    "functions": {
      "public/index.sh": {
        "memory": 1024,
        "maxDuration": 30,
        "runtime": "provided"
      }
    },
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/public/index.sh"
      }
    ],
    "env": {
      "NODE_ENV": "production",
      "GIT_TOKEN": ""
    }
  }